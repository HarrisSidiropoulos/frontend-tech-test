{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "main": "server.js",
  "scripts": {
    "pretest": "npm run generate:test",
    "test": "./node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/jest",
    "watch:test": "npm t -- --watch",
    "generate": "./node_modules/.bin/cross-env NODE_ENV=development node ./server/generate-json.js",
    "generate:test": "./node_modules/.bin/cross-env NODE_ENV=test node ./server/generate-json.js",
    "lint": "node_modules/.bin/eslint client, server",
    "prestart": "npm run generate",
    "start": "./node_modules/.bin/cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development ./node_modules/.bin/nodemon index.js",
    "build": "./node_modules/.bin/webpack --config ./webpack.config.js"
  },
  "keywords": [
    "test",
    "travix"
  ],
  "author": "spartans",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "./node_modules/",
      ".+\\.json$"
    ],
    "watchPathIgnorePatterns": [
      "./node_modules/",
      ".+\\.json$"
    ]
  },
  "dependencies": {
    "express": "^4.14.0",
    "frint": "2.4.1",
    "frint-react": "2.4.1",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-webpack-loaders": "0.9.0",
    "babel-preset-travix": "1.1.1",
    "chai": "4.1.2",
    "cross-env": "5.0.5",
    "dummy-json": "2.0.0",
    "eslint": "4.6.1",
    "eslint-config-travix": "3.0.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.3.0",
    "jest": "21.0.1",
    "nodemon": "1.12.0",
    "supertest": "3.0.0",
    "webpack": "3.5.6"
  }
}
