{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/jest",
    "watch:test": "npm t -- --watch",
    "start": "./node_modules/.bin/cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development ./node_modules/.bin/nodemon index.js"
  },
  "keywords": [
    "test",
    "travix"
  ],
  "author": "spartans",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns" : ["./node_modules/", ".+\\.json$" ],
    "watchPathIgnorePatterns" : ["./node_modules/", ".+\\.json$" ]
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-webpack-loaders": "0.9.0",
    "babel-preset-travix": "1.1.1",
    "chai": "4.1.2",
    "cross-env": "5.0.5",
    "eslint": "4.6.1",
    "eslint-config-travix": "3.0.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.3.0",
    "jest": "21.0.1",
    "nodemon": "1.12.0",
    "supertest": "3.0.0"
  }
}
